{
  "name": "andreas-glaser/doctrine-rql",
  "description": "Parses and applies RQL/FIQL scripts to doctrine orm query builders",
  "keywords": [
    "php",
    "doctrine",
    "database",
    "rql",
    "fiql",
    "api"
  ],
  "homepage": "https://github.com/andreas-glaser/doctrine-rql",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Glaser",
      "email": "andreas.c.glaser@gmail.com",
      "homepage": "http://andreas.glaser.me"
    }
  ],
  "require": {
    "php": "^7.2",
    "andreas-glaser/php-helpers": "^1",
    "doctrine/orm": "^2.4.8",
    "graviton/rql-parser": "^3"
  },
  "require-dev": {
    "doctrine/data-fixtures": "@stable",
    "phpunit/phpunit": "^7",
    "symfony/var-dumper": "@stable"
  },
  "suggest": {},
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "AndreasGlaser\\DoctrineRql\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AndreasGlaser\\DoctrineRql\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev"
    }
  }
}
