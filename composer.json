{
  "name": "andreas-glaser/doctrine-rql",
  "description": "Parses and applies RQL/FIQL scripts to doctrine orm query builders",
  "keywords": [
    "php",
    "doctrine",
    "database",
    "rql",
    "fiql",
    "api"
  ],
  "homepage": "https://github.com/andreas-glaser/doctrine-rql",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Glaser",
      "email": "andreas.glaser@pm.me",
      "homepage": "https://github.com/andreas-glaser"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "andreas-glaser/php-helpers": "^1.2.0",
    "doctrine/orm": "^2.4.8",
    "graviton/rql-parser": "^3"
  },
  "require-dev": {
    "ext-sqlite3": "*",
    "doctrine/data-fixtures": "^1.3.1",
    "phpunit/phpunit": "^8",
    "friendsofphp/php-cs-fixer": "@stable",
    "symfony/var-dumper": "^4.3.2"
  },
  "suggest": {},
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "AndreasGlaser\\DoctrineRql\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AndreasGlaser\\DoctrineRql\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4-dev"
    }
  }
}
